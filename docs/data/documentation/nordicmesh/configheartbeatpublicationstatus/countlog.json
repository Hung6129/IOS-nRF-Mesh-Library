{"schemaVersion":{"patch":0,"minor":3,"major":0},"kind":"symbol","sections":[],"abstract":[{"text":"Number of Heartbeat messages remaining to be sent.","type":"text"}],"metadata":{"roleHeading":"Instance Property","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"text":"countLog","kind":"identifier"},{"text":": ","kind":"text"},{"text":"UInt8","preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier"}],"symbolKind":"property","role":"symbol","modules":[{"name":"NordicMesh"}],"externalID":"s:10NordicMesh32ConfigHeartbeatPublicationStatusV8countLogs5UInt8Vvp","title":"countLog"},"hierarchy":{"paths":[["doc:\/\/NordicMesh\/documentation\/NordicMesh","doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus"]]},"identifier":{"url":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus\/countLog","interfaceLanguage":"swift"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"countLog","kind":"identifier"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"UInt8","preciseIdentifier":"s:s5UInt8V"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"content","content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"inlineContent":[{"text":"Possible values:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"0x00 - Periodic Heartbeat messages are not published.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"0x01 - 0x11 - Number of Heartbeat messages, 2^(n-1), that remain to be sent."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"0xFF - Periodic Heartbeat messages are published sent indefinitely.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Other values are prohibited.","type":"text"}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"The Heartbeat Publication Count Log is a representation of the Heartbeat Publication","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Count state value. The Heartbeat Publication Count Log and Heartbeat Publication Count"},{"type":"text","text":" "},{"text":"with the value 0x00 and 0x0000 are equivalent. The Heartbeat Publication Count Log","type":"text"},{"type":"text","text":" "},{"text":"value of 0xFF is equivalent to the Heartbeat Publication count value of 0xFFFF.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"The Heartbeat Publication Count Log value between 0x01 and 0x11 shall represent that"},{"type":"text","text":" "},{"text":"smallest integer n where 2^(n-1) is greater than or equal to the Heartbeat Publication","type":"text"},{"type":"text","text":" "},{"type":"text","text":"Count value. For example, if the Heartbeat Publication Count value is 0x0579, then the"},{"text":" ","type":"text"},{"text":"Heartbeat Publication Count Log value would be 0x0C.","type":"text"}],"type":"paragraph"}]}],"variants":[{"paths":["\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"doc://NordicMesh/documentation/NordicMesh/ConfigHeartbeatPublicationStatus":{"identifier":"doc:\/\/NordicMesh\/documentation\/NordicMesh\/ConfigHeartbeatPublicationStatus","url":"\/documentation\/nordicmesh\/configheartbeatpublicationstatus","type":"topic","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"ConfigHeartbeatPublicationStatus","kind":"identifier"}],"title":"ConfigHeartbeatPublicationStatus","kind":"symbol","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"ConfigHeartbeatPublicationStatus"}],"abstract":[]},"doc://NordicMesh/documentation/NordicMesh/ConfigHeartbeatPublicationStatus/countLog":{"url":"\/documentation\/nordicmesh\/configheartbeatpublicationstatus\/countlog","type":"topic","identifier":"doc:\/\/NordicMe